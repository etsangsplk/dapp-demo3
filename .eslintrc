{
  "parser": "babel-eslint",
  "rules": {
    "max-len": [2, 120],
    "func-names": 0,
    "prefer-arrow-callback": 0,
    "jsx-a11y/no-static-element-interactions": 0,
    "react/prefer-stateless-function": 0,
    "react/prop-types": 0
  },
  "extends": "airbnb",
  "plugins": [
    "react",
    "jsx-a11y",
    "import"
  ],
  "env": {
    "browser": true
  }
}
